{
    "name": "m6web/statsd-bundle",
    "description" : "Add statds metrics in your symfony app. Allow you to bind metrics on any symfony event.",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "bundle", "statsd", "m6web"],
    "authors": [
        {
            "name": "M6Web",
            "email": "opensource@m6web.fr",
            "homepage": "http://tech.m6web.fr/"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "vendor"
    },
    "require": {
        "php": "^7.1.3",
        "symfony/property-access": "^3.4 || ^4.0",
        "m6web/statsd": "^1.3"
    },
    "require-dev": {
        "atoum/atoum": "^2.8|^3.0",
        "symfony/symfony": "^3.4 || ^4.0 || ^5.0",
        "m6web/coke" : "~1.2",
        "m6web/symfony2-coding-standard" : "~1.2"
    },
    "autoload": {
        "psr-4": { "M6Web\\Bundle\\StatsdBundle\\": "src/" }
    }
}
